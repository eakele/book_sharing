<div class="page-header"><h1>Sign Up Page</h1></div>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name),html: {class: 'form-horizontal'}) do |f| %>
<%= devise_error_messages! %>
<div class="field form-group">
 <%= f.label :username,class:'col-sm-4 control-label' %>
 <div class="col-sm-4">
 <%= f.text_field :username, class: "form-control" %>
</div>
</div>

 <div class="field form-group">
<%= f.label :email ,class:'col-sm-4 control-label'%>
<div class="col-sm-4">
<%= f.email_field :email, class: "form-control" %>
</div>
</div>

<div class="field form-group">
    <%= f.label :first_name ,class:'col-sm-4 control-label'%>
    <div class="col-sm-4">
    <%= f.text_field :first_name , class: "form-control"%>
  </div>
</div>

  <div class="field form-group">
    <%= f.label :last_name,class: 'col-sm-4 control-label'%>
    <div class="col-sm-4">
    <%= f.text_field :last_name, class: "form-control" %>
  </div>
</div>

<div class="field form-group">
  <%= f.label :password ,class:'col-sm-4 control-label'%>
  <div class="col-sm-4">
<%= f.password_field :password, class: "form-control" %>
</div>
</div>

<div class="field form-group">
<%= f.label :password_confirmation ,class:'col-sm-4 control-label'%>
<div class="col-sm-4">
<%= f.password_field :password_confirmation , class: "form-control"%>
</div>
</div>

<div class="actions form-group">
  <div class="col-sm-offset-4 col-sm-4">
<%= f.submit "Sign up",class: "btn btn-default btn-primary"  %>
</div>
</div>
<% end %>

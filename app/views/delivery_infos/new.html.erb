<div class="page-header"><h1>Delivery registration</h1>
  <h4>Welcome... <%= current_user.first_name+" "+current_user.last_name%></h4>
</div>

<%=form_for :delivery_info, url: '/delivery',html: {class: 'form-horizontal'} do |f| %>
<%= render 'shared/errors', object: @deliveryInfo %>

<div class="field form-group">
     <%=f.label :delivered_to,'Accepted By',class: 'col-sm-4 control-label' %>
     <div class="col-sm-4">
     <%=f.text_field :delivered_to, class: "form-control"%>
     </div>
</div>

<div class="field form-group">
    <%=f.label :image,'Attach Documentation',class: "col-sm-4 control-label" %>
     <div class="col-sm-4">
     <%= f.file_field :image, class: "form-control" %>
</div>
</div>
<div class="actions form-group">
  <div class="col-sm-offset-4 col-sm-6">
    <%=f.submit "Save",class: "btn btn-default btn-primary" %>
  </div>
</div>
<% end %>
